model Profile {
  id           Int            @id @default(autoincrement())
  profileName  String         @map("profile_name") @db.VarChar(50)
  publicId     String?        @unique @map("public_id") @db.VarChar(40)
  type         ProfileType    @default(ADVOGADO)
  User         User[]
  ProfileRoute ProfileRoute[]

  @@map("profiles")
}
